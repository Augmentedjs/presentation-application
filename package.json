{
  "name": "presentation-application",
  "version": "2.8.1",
  "description": "The Augmented.js Next - Presentation Application Module.",
  "main": "dist/presentation-application.js",
  "scripts": {
    "dev": "webpack --mode development --module-bind js=babel-loader --progress",
    "build": "webpack --mode production --module-bind js=babel-loader --progress",
    "lint": "eslint ./src/presentation-application.js",
    "test": "mocha --require babel-core/register --require test/helper.js -c test/*Spec.js",
    "start": "node index.js",
    "docs": "rm -rf jsdoc && jsdoc ./src -r -d ./jsdoc/ -R ./README.md -t ./node_modules/foodoc/template -c ./jsdoc.conf.json -P ./package.json",
    "readme": "./node_modules/documentation/bin/documentation.js readme ./src/index.js -f md --section=API",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmentedjs/presentation-application.git"
  },
  "keywords": [
    "augmentedjs",
    "presentation"
  ],
  "author": "Bob Warren <jedi.master@augmentedjs.com> (http://www.augmentedjs.com)",
  "license": "Apache-2.0",
  "devDependencies": {
    "acorn": "^7.0.0",
    "acorn-dynamic-import": "^4.0.0",
    "ajv-errors": "^1.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "documentation": "^12.1.1",
    "eslint": "^6.2.0",
    "eslint-config-kentcdodds": "^14.3.4",
    "foodoc": "0.0.9",
    "jsdoc": "^3.6.3",
    "jsdom": "^15.1.1",
    "marked": "^0.7.0",
    "mocha": "^6.2.0",
    "mocha-jsdom": "^2.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "next-core-application": "^2.5.3",
    "next-core-structures": "^2.5.3",
    "presentation-router": "^2.6.7"
  },
  "bugs": {
    "url": "https://github.com/Augmentedjs/presentation-application/issues"
  },
  "homepage": "http://www.augmentedjs.com"
}
